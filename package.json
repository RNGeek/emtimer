{
  "name": "emtimer",
  "version": "1.0.0",
  "description": "Emtimer is a cowntdown timer, which is simple and high functional.",
  "author": "mizdra <pp.mizdra@gmail.com>",
  "private": true,
  "repository": "RNGeek/emtimer",
  "scripts": {
    "dev": "run-p -l -c dev:frontend dev:serverless",
    "dev:frontend": "webpack-dev-server --mode development --config webpack.frontend.js",
    "dev:serverless": "run-p dev:serverless:webpack dev:serverless:serve",
    "dev:serverless:webpack": "webpack --watch --mode development --config webpack.serverless.js",
    "dev:serverless:serve": "netlify-lambda serve . --static",
    "lint": "eslint --ext .js,.vue,.ts .",
    "typecheck": "run-p -l -c typecheck:*",
    "typecheck:frontend": "tsc -p tsconfig.frontend.json --noEmit",
    "typecheck:serverless": "tsc -p tsconfig.serverless.json --noEmit",
    "build": "run-s build:frontend build:serverless",
    "build:frontend": "webpack --mode production --config webpack.frontend.js",
    "build:serverless": "webpack --mode production --config webpack.serverless.js",
    "prebuild": "run-s typecheck lint"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^5.2.0",
    "@xkeshi/vue-countdown": "mizdra/vue-countdown",
    "bowser": "^2.10.0",
    "dotenv": "^8.2.0",
    "keycode": "^2.1.9",
    "muse-ui": "^2.1.0",
    "tslib": "^1.8.1",
    "typescript": "^4.0.2",
    "vue": "^2.6.12",
    "vue-router": "^2.7.0",
    "vue-snotify": "^3.0.4",
    "vuelidate": "^0.5.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.92",
    "@types/node": "^14.6.2",
    "@typescript-eslint/parser": "^3.10.1",
    "autoprefixer": "^9.8.6",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.6.0",
    "eslint": "^7.7.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.5.3",
    "netlify-lambda": "^2.0.15",
    "npm-run-all": "^4.1.2",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.3.0",
    "ts-loader": "^9.2.6",
    "url-loader": "^4.1.1",
    "vue-eslint-parser": "^7.1.0",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-node-externals": "^3.0.0",
    "workbox-webpack-plugin": "^6.4.2"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  }
}
