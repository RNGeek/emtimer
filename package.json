{
  "name": "emtimer",
  "version": "1.0.0",
  "description": "Emtimer is a cowntdown timer, which is simple and high functional.",
  "author": "mizdra <pp.mizdra@gmail.com>",
  "private": true,
  "repository": "RNGeek/emtimer",
  "scripts": {
    "dev": "run-p dev:app dev:server",
    "dev:app": "webpack-dev-server --mode development --config webpack.app.js",
    "dev:server": "run-p dev:server:webpack dev:server:serve",
    "dev:server:webpack": "webpack --watch --mode development --config webpack.server.js",
    "dev:server:serve": "netlify-lambda serve . --static",
    "lint": "eslint --ext .js,.vue,.ts .",
    "build": "run-s build:app build:server",
    "build:app": "webpack --mode production --config webpack.app.js",
    "build:server": "webpack --mode production --config webpack.server.js",
    "prebuild": "run-s lint"
  },
  "dependencies": {
    "@xkeshi/vue-countdown": "mizdra/vue-countdown",
    "bowser": "^2.10.0",
    "keycode": "^2.1.9",
    "muse-ui": "^2.1.0",
    "tslib": "^1.8.1",
    "typescript": "^4.0.2",
    "uuid": "^8.3.0",
    "vue": "^2.6.12",
    "vue-router": "^2.7.0",
    "vue-snotify": "^3.0.4",
    "vuelidate": "^0.5.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.61",
    "@types/node": "^14.6.2",
    "@typescript-eslint/parser": "^3.10.1",
    "autoprefixer": "^9.8.6",
    "copy-webpack-plugin": "^6.0.4",
    "css-loader": "^4.2.2",
    "eslint": "^7.7.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.11.0",
    "netlify-lambda": "^2.0.1",
    "npm-run-all": "^4.1.2",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "sw-precache-webpack-plugin": "^1.0.0",
    "ts-loader": "^8.0.3",
    "url-loader": "^4.1.0",
    "vue-eslint-parser": "^7.1.0",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  }
}
